{
  "name": "@ahao430/sdk-demo",
  "author": "ahao430",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.umd.js",
  "module": "dist/index.js",
  "scripts": {
    "start": "sirv 'demo' -I --no-clear",
    "dev": "npm run dev:h5",
    "dev:h5": "cross-env NODE_ENV=develop RUN_ENV=h5 rollup -c -w",
    "dev:zfb": "cross-env NODE_ENV=develop RUN_ENV=zfb rollup -c -w",
    "build": "npm run build:h5 && npm run build:zfb",
    "build:h5": "cross-env NODE_ENV=production RUN_ENV=h5 rollup -c",
    "build:zfb": "cross-env NODE_ENV=production RUN_ENV=zfb rollup -c",
    "build:release": "cross-env RELEASE=true npm run build:h5 && npm run build:zfb",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts}'",
    "test": "vitest -w",
    "test:coverage": "vitest run --coverage",
    "doc": "typedoc --options typedoc.json && npm run doc:gh-fix",
    "doc:gh-fix": "touch docs/.nojekyll",
    "publish": "release-it"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^3.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^16.7.8",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitest/coverage-c8": "^0.28.5",
    "@vitest/coverage-istanbul": "^0.28.5",
    "babel": "^6.23.0",
    "babel-eslint": "^10.1.0",
    "c8": "^7.13.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^26.6.3",
    "jsdom": "^21.1.0",
    "mini-types": "^0.1.7",
    "pre-commit": "^1.2.2",
    "release-it": "^15.9.3",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.2.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "sirv": "^2.0.2",
    "sirv-cli": "^2.0.0",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typedoc": "^0.19.2",
    "typedoc-neo-theme": "^1.0.10",
    "typescript": "^4.0.5",
    "vitest": "^0.28.5"
  },
  "license": "ISC",
  "dependencies": {
    "@types/qs": "^6.9.7",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "crypto-js": "^4.1.1",
